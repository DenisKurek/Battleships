<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="box" alignment="CENTER" fillWidth="false" prefHeight="1000.0" prefWidth="1000.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SecondaryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Your Board" textFill="#1dc626">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Pane fx:id="playerPane" prefHeight="400.0" prefWidth="400.0" />
      <Label text="Enemy Board" textFill="#dd1c1c">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Pane fx:id="enemyPane" prefHeight="400.0" prefWidth="400.0" />
   </children>
</VBox>
